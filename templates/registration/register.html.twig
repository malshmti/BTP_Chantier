{% extends 'base.html.twig' %}

{% block title %}Inscription{% endblock %}

{% block body %}
    <div class="container">
        <h1>Inscription</h1>

        {{ form_start(registrationForm) }}
            {{ form_row(registrationForm.email) }}
            {{ form_row(registrationForm.plainPassword, {
                label: 'Password'
            }) }}
            {{ form_row(registrationForm.typeActeur) }}

            <button type="submit" class="btn btn-light text-white nav-item nav-link signIn">Register</button>
        {{ form_end(registrationForm) }}

    </div>
{% endblock %}
